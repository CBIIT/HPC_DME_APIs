   SOURCE_LOCATION_FILE_ID clob,
   SOURCE_LOCATION_FILE_CONTAINER_ID clob,
   SOURCE_LOCATION_FILE_CONTAINER_NAME clob,
   CREATED timestamp NOT NULL,
   COMPLETED timestamp NOT NULL
)
;
CREATE UNIQUE INDEX HPC_DATA_OBJECT_REGISTRATION_RESULT_pkey ON "public"."HPC_DATA_OBJECT_REGISTRATION_RESULT"(ID)
;
