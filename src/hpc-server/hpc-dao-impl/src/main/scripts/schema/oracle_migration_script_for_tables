CREATE TABLE "public"."HPC_NOTIFICATION_DELIVERY_RECEIPT"
(
   EVENT_ID number(10) NOT NULL,
   USER_ID clob NOT NULL,
   NOTIFICATION_DELIVERY_METHOD clob NOT NULL,
   DELIVERY_STATUS char(1) NOT NULL,
   DELIVERED timestamp NOT NULL,
   CONSTRAINT HPC_NOTIFICATION_DELIVERY_RECEIPT_pkey PRIMARY KEY (EVENT_ID,USER_ID,NOTIFICATION_DELIVERY_METHOD)
)
;
CREATE UNIQUE INDEX HPC_NOTIFICATION_DELIVERY_RECEIPT_pkey ON "public"."HPC_NOTIFICATION_DELIVERY_RECEIPT"
(
  EVENT_ID,
  USER_ID,
  NOTIFICATION_DELIVERY_METHOD
)
;
CREATE TABLE "public"."HPC_NOTIFICATION_SUBSCRIPTION"
(
   ID number(10) PRIMARY KEY NOT NULL,
   USER_ID clob NOT NULL,
   EVENT_TYPE clob NOT NULL,
   NOTIFICATION_DELIVERY_METHODS _text NOT NULL
)
;
CREATE UNIQUE INDEX HPC_NOTIFICATION_SUBSCRIPTION_pkey ON "public"."HPC_NOTIFICATION_SUBSCRIPTION"(ID)
;
CREATE UNIQUE INDEX HPC_NOTIFICATION_SUBSCRIPTION_unique ON "public"."HPC_NOTIFICATION_SUBSCRIPTION"
(
  USER_ID,
  EVENT_TYPE
)
;
CREATE TABLE "public"."HPC_NOTIFICATION_TRIGGER"
(
   NOTIFICATION_SUBSCRIPTION_ID number(10),
   NOTIFICATION_TRIGGER _text
)
;
ALTER TABLE "public"."HPC_NOTIFICATION_TRIGGER"
ADD CONSTRAINT HPC_NOTIFICATION_TRIGGER_NOTIFICATION_SUBSCRIPTION_ID_fkey
FOREIGN KEY (NOTIFICATION_SUBSCRIPTION_ID)
REFERENCES "public"."HPC_NOTIFICATION_SUBSCRIPTION"(ID) ON DELETE CASCADE ON UPDATE CASCADE
;
