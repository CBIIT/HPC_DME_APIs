CREATE TABLE "public"."HPC_BULK_DATA_OBJECT_REGISTRATION_RESULT"
(
   ID clob PRIMARY KEY NOT NULL,
   USER_ID clob NOT NULL,
   RESULT char(1) NOT NULL,
   MESSAGE clob,
   ITEMS clob,
   CREATED timestamp NOT NULL,
   COMPLETED timestamp NOT NULL,
   EFFECTIVE_TRANSFER_SPEED number(10)
)
;
CREATE UNIQUE INDEX HPC_BULK_DATA_OBJECT_REGISTRATION_RESULT_pkey ON "public"."HPC_BULK_DATA_OBJECT_REGISTRATION_RESULT"(ID)
;
CREATE TABLE "public"."HPC_BULK_DATA_OBJECT_REGISTRATION_TASK"
(
   ID clob PRIMARY KEY NOT NULL,
   USER_ID clob,
   STATUS clob,
   ITEMS clob,
   CREATED timestamp,
   UI_URL clob
)
;
CREATE UNIQUE INDEX HPC_BULK_DATA_OBJECT_REGISTRATION_TASK_pkey ON "public"."HPC_BULK_DATA_OBJECT_REGISTRATION_TASK"(ID)
;
CREATE TABLE "public"."HPC_CATALOG_ATTRIBUTE"
(
   level_label clob NOT NULL,
   meta_attr_name clob NOT NULL,
   CONSTRAINT HPC_CATALOG_ATTRIBUTE_pkey PRIMARY KEY (level_label,meta_attr_name)
)
;
