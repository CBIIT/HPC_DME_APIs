(
  level_label,
  meta_attr_name
)
;
CREATE TABLE "public"."HPC_COLLECTION_DOWNLOAD_TASK"
(
   ID clob PRIMARY KEY NOT NULL,
   USER_ID clob,
   TYPE clob NOT NULL,
   PATH clob,
   CONFIGURATION_ID clob,
   DATA_OBJECT_PATHS _text,
   DESTINATION_LOCATION_FILE_CONTAINER_ID clob,
   DESTINATION_LOCATION_FILE_ID clob,
   DESTINATION_OVERWRITE char(1),
   S3_ACCOUNT_ACCESS_KEY blob,
   S3_ACCOUNT_SECRET_KEY blob,
   S3_ACCOUNT_REGION clob,
   STATUS clob,
   ITEMS clob,
   CREATED timestamp,
   PRIORITY number(10) DEFAULT 100,
   COLLECTION_PATHS _text,
   APPEND_PATH_TO_DOWNLOAD_DESTINATION char(1),
   CANCELLATION_REQUESTED char(1),
   GOOGLE_DRIVE_ACCESS_TOKEN blob,
   IN_PROCESS char(1) DEFAULT false NOT NULL
)
;
CREATE UNIQUE INDEX HPC_COLLECTION_DOWNLOAD_TASK_pkey ON "public"."HPC_COLLECTION_DOWNLOAD_TASK"(ID)
;
CREATE TABLE "public"."HPC_DATA_MANAGEMENT_AUDIT"
(
   USER_ID clob NOT NULL,
   PATH clob NOT NULL,
   REQUEST_TYPE clob NOT NULL,
   METADATA_BEFORE clob NOT NULL,
   METADATA_AFTER clob,
   ARCHIVE_FILE_CONTAINER_ID clob,
   ARCHIVE_FILE_ID clob,
   DATA_MANAGEMENT_STATUS char(1) NOT NULL,
   DATA_TRANSFER_STATUS char(1),
   MESSAGE clob,
   COMPLETED timestamp NOT NULL
)
;
CREATE TABLE "public"."HPC_DATA_MANAGEMENT_CONFIGURATION"
(
   ID clob PRIMARY KEY NOT NULL,
   BASE_PATH clob NOT NULL,
   DOC clob NOT NULL,
   DATA_HIERARCHY clob,
   COLLECTION_METADATA_VALIDATION_RULES clob,
   DATA_OBJECT_METADATA_VALIDATION_RULES clob,
   GLOBUS_URL clob NOT NULL,
   GLOBUS_ARCHIVE_ENDPOINT clob NOT NULL,
   GLOBUS_ARCHIVE_PATH clob NOT NULL,
   GLOBUS_ARCHIVE_DIRECTORY clob NOT NULL,
   GLOBUS_ARCHIVE_TYPE clob NOT NULL,
   GLOBUS_DOWNLOAD_ENDPOINT clob,
   GLOBUS_DOWNLOAD_PATH clob,
   GLOBUS_DOWNLOAD_DIRECTORY clob,
   S3_UPLOAD_ARCHIVE_CONFIGURATION_ID clob,
   S3_DEFAULT_DOWNLOAD_ARCHIVE_CONFIGURATION_ID clob
)
;
CREATE UNIQUE INDEX HPC_DATA_MANAGEMENT_CONFIGURATION_pkey ON "public"."HPC_DATA_MANAGEMENT_CONFIGURATION"(ID)
;
CREATE TABLE "public"."HPC_DATA_OBJECT_DOWNLOAD_TASK"
(
   ID clob PRIMARY KEY NOT NULL,
   USER_ID clob,
   PATH clob,
   CONFIGURATION_ID clob,
   DATA_TRANSFER_REQUEST_ID clob,
   DATA_TRANSFER_TYPE clob,
   DATA_TRANSFER_STATUS clob,
   DOWNLOAD_FILE_PATH clob,
   ARCHIVE_LOCATION_FILE_CONTAINER_ID clob,
   ARCHIVE_LOCATION_FILE_ID clob,
   DESTINATION_LOCATION_FILE_CONTAINER_ID clob,
   DESTINATION_LOCATION_FILE_ID clob,
   COMPLETION_EVENT char(1),
   PERCENT_COMPLETE number(10),
   SIZE number(19),
   CREATED timestamp,
   DESTINATION_TYPE clob,
   S3_ACCOUNT_ACCESS_KEY blob,
   S3_ACCOUNT_SECRET_KEY blob,
   S3_ACCOUNT_REGION clob,
   PRIORITY number(10) DEFAULT 100,
   S3_ARCHIVE_CONFIGURATION_ID clob,
   PROCESSED timestamp,
   GOOGLE_DRIVE_ACCESS_TOKEN blob
)
;
CREATE UNIQUE INDEX HPC_DATA_OBJECT_DOWNLOAD_TASK_pkey ON "public"."HPC_DATA_OBJECT_DOWNLOAD_TASK"(ID)
;
