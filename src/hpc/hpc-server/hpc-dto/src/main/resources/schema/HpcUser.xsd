<xsd:schema xmlns:xsd="http://www.w3.org/2001/XMLSchema"
	xmlns:jxb="http://java.sun.com/xml/ns/jaxb" jxb:version="1.0"
	xmlns:hpc-domain-user="http://hpc.nci.nih.gov/domain/user"
	targetNamespace="http://hpc.nci.nih.gov/dto/user" elementFormDefault="unqualified">

	<xsd:import namespace="http://hpc.nci.nih.gov/domain/user"
		schemaLocation="HpcUserTypes.xsd" />

	<!-- User DTO -->
	<xsd:element name="HpcUserRegistrationDTO">
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element name="nihAccount" type="hpc-domain-user:HpcNihAccount" />
				<xsd:element name="dataTransferAccount"
					type="hpc-domain-user:HpcDataTransferAccount" />
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>

	<!-- User DTO -->
	<xsd:element name="HpcUserDTO">
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element name="nihAccount" type="hpc-domain-user:HpcNihAccount" />
				<xsd:element name="dataTransferAccount"
					type="hpc-domain-user:HpcDataTransferAccount" />
				<xsd:element name="created" type="xsd:dateTime" />
				<xsd:element name="lastUpdated" type="xsd:dateTime" />
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>

</xsd:schema>
