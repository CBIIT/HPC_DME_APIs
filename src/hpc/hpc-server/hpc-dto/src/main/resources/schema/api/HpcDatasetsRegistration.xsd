<xsd:schema xmlns:xsd="http://www.w3.org/2001/XMLSchema"
			xmlns:jxb="http://java.sun.com/xml/ns/jaxb" jxb:version="1.0"
			xmlns:hpc="http://hpc.nci.nih.gov/dto/types"
    		targetNamespace="http://hpc.nci.nih.gov/dto/api"
    		elementFormDefault="qualified">
    		
   	<xsd:import namespace="http://hpc.nci.nih.gov/dto/types" schemaLocation="../types/HpcDatasetTypes.xsd"/>
    
    <xsd:element name="HpcDatasetsRegistrationInputDTO">
        <xsd:complexType>
        	<xsd:sequence>
        		<xsd:element name="datasets" type="hpc:HpcDataset" minOccurs="1" maxOccurs="unbounded"/>
        	</xsd:sequence>
        </xsd:complexType>
    </xsd:element>
</xsd:schema>
