<xsd:schema xmlns:xsd="http://www.w3.org/2001/XMLSchema"
			xmlns:jxb="http://java.sun.com/xml/ns/jaxb" jxb:version="1.0"
			xmlns:hpc-domain="http://hpc.nci.nih.gov/domain"
    		targetNamespace="http://hpc.nci.nih.gov/dto"
    		elementFormDefault="unqualified">
    		
   	<xsd:import namespace="http://hpc.nci.nih.gov/domain" schemaLocation="HpcDataTypes.xsd"/>
    
    <!--  Registration Service Input DTO -->
    <xsd:element name="HpcDataRegistrationInput">
        <xsd:complexType>
  			<xsd:sequence>
  				<xsd:element name="type" type="hpc-domain:HpcManagedDataType"/>
        		<xsd:element name="datasets" type="hpc-domain:HpcDataset" 
			    	         minOccurs="1" maxOccurs="unbounded"/>
        	</xsd:sequence>
        	
        </xsd:complexType>
    </xsd:element>
    
    <!--  Registration Service Output DTO -->
    <xsd:element name="HpcDataRegistrationOutput">
    	<xsd:complexType>
  			<xsd:sequence>
  			  	<xsd:element name="type" type="hpc-domain:HpcManagedDataType"/>
  				<xsd:element name="created" type="xsd:dateTime"/>
        		<xsd:element name="datasets" type="hpc-domain:HpcDataset" 
			    	         minOccurs="1" maxOccurs="unbounded"/>
  			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>
</xsd:schema>
