{
	"HpcMetadataValidationRules":{
		"collectionMetadataValidationRules":[
			{
				"attribute": "collection_type",
				"mandatory": true,
				"validValues":[
					"ParentColllection",
					"SubCollection"
				],
				"ruleEnabled": true,
				"docs":["UNIT_TEST"]
			},
			{
				"attribute": "text",
				"mandatory": true,
				"collectionTypes":[
					"ParentColllection",
					"SubCollection"
				],
				"ruleEnabled":true,
				"docs":["UNIT_TEST"]
			},
			{
				"attribute": "choice",
				"mandatory": false,
				"defaultValue":"A",
				"collectionTypes":[
					"ParentColllection",
					"SubCollection"
				],
				"validValues":[
					"A",
					"B"
				],
				"ruleEnabled":true,
				"docs":["UNIT_TEST"]
			}
		],
		"dataObjectMetadataValidationRules":[
